# created a list of paths to update registry values in 

$paths = ("HKLM:\SOFTWARE\Classes\batfile\shell\runasuser\","HKLM:\SOFTWARE\Classes\cmdfile\shell\runasuser\","HKLM:\SOFTWARE\Classes\exefile\shell\runasuser\","HKLM:\SOFTWARE\Classes\mscfile\shell\runasuser\")

# iterated through each path and created it if didnt exist 
foreach( $path in $paths){
    if (-not (Test-Path $path)){
    New-Item -Path $path -Force | Out-Null

}

# added the "suppression policy key"
Set-ItemProperty -Path $path  -Name SuppressionPolicy -value 0x00001000 -Type Dword
Get-ItemProperty -Path $path | Select-Object SuppressionPolicy
} 
